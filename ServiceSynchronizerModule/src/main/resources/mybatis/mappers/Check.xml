<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Check">

    <select id="offline" resultType="String">
        select collectorId from tb_HeartBeat
        where
            current_timestamp - tb_HeartBeat.time &gt; 0
    </select>

</mapper>