<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Data">

    <insert id="add" parameterType="team.hnuwt.bean.Meter">
        insert into tb_data(id,data,valveState)
        values(#{id},#{data},#{valveState})
    </insert>
    
    <insert id="addBatch">
        insert into tb_data(id,data,valveState)
        values
        <foreach collection="list" item="data" separator=",">
            (#{data.id},#{data.data},#{data.valveState})
        </foreach>
    </insert>
    
</mapper>